    "build-external": "webpack --config ./external/webpack.config.js --mode production",
    "build-multi": "webpack --config ./multi-entry-points/webpack.config.js --mode production",
    "build-css": "webpack --config ./css-style-loader/webpack.config.js --mode production",
    "build-plugins": "webpack --config ./plugins/webpack.config.js --mode production",
    "build-dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js --mode production",
    "build-babel": "webpack-dev-server --config ./babel/webpack.config.js --mode production",
    "build-react": "webpack-dev-server --config ./react/webpack.config.js --mode production",
    "build-url-loader": "webpackdev-server --config ./url-loader/webpack.config.js --mode production",
    "build-prepro": "webpackdev-server --config ./preprocesadores/webpack.config.js --mode production",
    "build-prevent-duplication": "webpack --config ./prevent-duplication/webpack.config.js --mode production"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@mikef123",
